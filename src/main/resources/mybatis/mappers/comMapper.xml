<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.jsinc.mybatis.community">
	<insert id="input" parameterType="dto_com">
	insert into createcom (empNo,name,title,content,com_date)values(#{empNo},#{name},#{title},#{content},#{com_date})
	
	</insert>
	
	<select id="getAll" resultType="dto_com">
		select * from createcom
	</select>
	
	<select id="myCom" resultType="dto_com">
		select * from createcom where empno=#{empNo}	
	</select>
	
	
</mapper>
